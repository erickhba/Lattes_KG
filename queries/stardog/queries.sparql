PREFIX : <file:///home/erick_albuquerque/Documents/TCC/owlReady/ontology.owl#>

SELECT ?orientacao ?instituicao ?tipoTrabalho ?statusOrientacao ?ano ?nome
WHERE {
  ?pesquisador :Nome "Gisele da Silva Craveiro" .
  ?pesquisador :orienta ?orientacao .
  ?orientacao :Instituicao ?instituicao .
  ?orientacao :TipoTrabalho ?tipoTrabalho .
  ?orientacao :StatusOrientacao ?statusOrientacao .
  ?orientacao :Ano ?ano .
  ?orientacao :Nome ?nome .
} ORDER BY ?ano


