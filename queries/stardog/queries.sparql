PREFIX : <file:///home/erick_albuquerque/Documents/TCC/Lattes_KG/OWL/ontology.owl#>

SELECT ?pesquisador ?areaAtuacao ?atuacao ?orientacao ?formacao
WHERE {
  ?pesquisador a :Pesquisador.
  OPTIONAL {
    ?pesquisador :areas_relacionadas ?areaAtuacao.
  }
  OPTIONAL {
    ?pesquisador :atua_em ?atuacao.
  }
  OPTIONAL {
    ?pesquisador :orienta ?orientacao.
  }
  OPTIONAL {
    ?pesquisador :possui_formacao ?formacao.
  }
}

